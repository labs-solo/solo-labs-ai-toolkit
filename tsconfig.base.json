{
  "compilerOptions": {
    "composite": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": ["es2022"],
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022",
    "customConditions": ["@solo-labs/source"],
    "baseUrl": ".",
    "paths": {
      "@solo-labs/utils": ["packages/utils/src/index.ts"],
      "@solo-labs/agents-agnostic": ["packages/agents/agnostic/src/index.ts"],
      "@solo-labs/commands-agnostic": [
        "packages/commands/agnostic/src/index.ts"
      ],
      "@solo-labs/protocol-knowledge": [
        "packages/agents/protocol-knowledge/src/index.ts"
      ],
      "@solo-labs/ai-toolkit-nx-claude": [
        "packages/ai-toolkit-nx-claude/src/index.ts"
      ]
    }
  }
}
